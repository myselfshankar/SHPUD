        $.ajax({
            type: 'POST',
            url: window.location.origin + "/Admin/Dashboard",
            data: '',
            beforeSend: function () {
                // setting a timeout
                $("#loading").show();
            },
            success: function (data) {
                // $("#viewlist")[0].innerHtml = data;
                //or
                $("#manageDiseaseView").html(data);
                $('#default').hide();
            },
            complete: function () {
                // setting a timeout
                $("#loading").hide();
            }
        });
        return false; //prevent default action(submit) for a button
    }
